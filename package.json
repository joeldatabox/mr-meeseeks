    "packagr": "ng-packagr -p ng-package.json",
    "start": "ng serve",
    "build": "ng build",
    "test": "rm -rf `find ./ -iname '*.tgz'` && rm -rf dist/ node_modules/ .ng_pkg_build/ package-lock.json && npm install &&  npm-install-peers && ng-packagr -p ng-package.json && npm pack ",
